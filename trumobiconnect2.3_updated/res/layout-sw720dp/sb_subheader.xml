<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/sb_subheadershadow"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/subheaderpopup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#dfdfdf"
        android:orientation="vertical" >

       <LinearLayout
            android:id="@+id/firstrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#fff"
            android:orientation="horizontal" >

            <RelativeLayout
                android:id="@+id/addressbarlayout"
                android:layout_width="wrap_content"
                android:layout_height="52dp"                
                android:gravity="center_vertical"
                android:visibility="visible" >               
                <ImageView
                    android:id="@+id/leftarrow"
                    android:layout_width="102dp"
                    android:layout_height="52dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:contentDescription="@string/more"
                    android:src="@drawable/sb_prev_icon"
                    android:visibility="invisible" />
                 <ImageView
                    android:id="@+id/leftarrowdisable"
                    android:layout_width="102dp"
                    android:layout_height="52dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"                    
                    android:src="@drawable/sb_prev_icon_disable"
                    android:visibility="visible" />                 
                 </RelativeLayout>
                 <ImageView
                	android:id="@+id/line1"
                	android:layout_width="1dp"
                	android:layout_height="52dp"
                	android:contentDescription="@string/more"
                	android:src="@drawable/sb_68x52" />
              <RelativeLayout
                android:id="@+id/addressbarlayout1"
                android:layout_width="wrap_content"
                android:layout_height="52dp"
                android:gravity="center_vertical"
                android:visibility="visible" >
                 <ImageView
                	android:id="@+id/rightarrow"
                	android:layout_width="102dp"
                	android:layout_height="52dp"                	               	
                	android:src="@drawable/sb_fwd_icon"
                	android:visibility="invisible" />
            	<ImageView
                	android:id="@+id/rightarrowdisable"
                	android:layout_width="102dp"
                	android:layout_height="52dp"
                	android:contentDescription="@string/more"
                	android:src="@drawable/sb_fwd_icon_disable"
                	android:visibility="visible" />            	 		
            </RelativeLayout>         
         </LinearLayout>
          <LinearLayout
            android:id="@+id/firstrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#fff"
            android:orientation="horizontal" 
            android:padding="0dp">
         <ImageView
                	android:id="@+id/horizontal"
                	android:layout_width="wrap_content"
                	android:layout_height="1dp"                	               	
                	android:src="@drawable/sb_horz_line"                	
                	 />
         </LinearLayout> 
         <LinearLayout
            android:id="@+id/secondrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#fff"
            android:orientation="horizontal" >			
             <ImageView
                android:id="@+id/download"
                android:layout_width="102dp"
                android:layout_height="52dp"
                android:contentDescription="@string/more"
                android:src="@drawable/sb_downloads_icon" /> 
             <ImageView
                	android:id="@+id/line12"
                	android:layout_width="1dp"
                	android:layout_height="52dp"
                	android:contentDescription="@string/more"
                	android:src="@drawable/sb_68x52" />
             <RelativeLayout android:id="@+id/addressbarlayout_fav"
                android:layout_width="wrap_content"
                android:layout_height="52dp"                
                android:gravity="center_vertical"
                android:visibility="visible" 
                 >
                  <ImageView
                android:id="@+id/favorite"
                android:layout_width="102dp"
                android:layout_height="52dp"
                android:contentDescription="@string/more"
                android:src="@drawable/sb_star_icon" 
                android:visibility="visible"/> 
                <ImageView
                android:id="@+id/favorite_selected"
                android:layout_width="68dp"
                android:layout_height="52dp"
                android:contentDescription="@string/more"
                android:src="@drawable/sb_star_icon_selected"
                android:visibility="invisible" />                 
             </RelativeLayout>		              
            
        </LinearLayout>        
		<TextView
            android:id="@+id/home"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="1dp"
            android:background="#fff"
            android:gravity="center_vertical"
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingTop="10dp"
            android:text="@string/home"
            android:textSize="18sp"
            android:textColor="#000000" 
            android:clickable="true"/>
        <TextView
            android:id="@+id/bookmarks"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="1dp"
            android:background="#fff"
            android:gravity="center_vertical"
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingTop="10dp"
            android:text="@string/bookmarks"
            android:textSize="18sp"
            android:textColor="#000000" 
            android:clickable="true"/>

        <TextView
            android:id="@+id/history"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="1dp"
            android:background="#fff"
            android:gravity="center_vertical"
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingTop="10dp"
            android:text="@string/history"
            android:textSize="18sp"
            android:textColor="#000000"
            android:clickable="true" />

        <TextView
            android:id="@+id/findinpages"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="1dp"
            android:background="#fff"
            android:gravity="center_vertical"
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingTop="10dp"
            android:text="@string/find_in_page"
            android:textSize="18sp"
            android:textColor="#000000"
            android:clickable="true" />        
        <TextView
            android:id="@+id/settings"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="1dp"
            android:background="#fff"
            android:gravity="center_vertical"
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingTop="10dp"
            android:text="@string/settings"
            android:textSize="18sp"
            android:textColor="#000000"
            android:clickable="true" />
         <TextView
            android:id="@+id/proxysetting"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="1dp"
            android:background="#fff"
            android:gravity="center_vertical"
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingTop="10dp"
            android:text="@string/proxy"
            android:textSize="18sp"
            android:textColor="#000000"
            android:clickable="true" 
            android:visibility="invisible"/>
    </LinearLayout>

</LinearLayout>